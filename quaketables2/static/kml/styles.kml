<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
	<Document>
		<name>QuakeSim KML Style File</name>
	  <Style id="s_dot">
    <IconStyle>
	<Icon><href>http://maps.google.com/mapfiles/kml/shapes/shaded_dot.png</href></Icon>
	<color>ffff0000</color>
	<scale>0.5</scale>
    </IconStyle>
    <LabelStyle>
      <color>00000000</color>
      <scale>0.000000</scale>
    </LabelStyle>
  </Style>

		<Style id="s_marker">
			<IconStyle>
				<scale>1</scale>
				<Icon>
					<href>http://www.google.com/mapfiles/marker.png</href>
				</Icon>
			</IconStyle>
		</Style>
		<Style id="s_ylw-pushpin_0">
			<IconStyle>
				<scale>1.1</scale>
				<Icon>
					<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
				</Icon>
				<hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
			</IconStyle>
		</Style>
		<Style id="s_ylw-pushpin_1">
			<IconStyle>
				<scale>1.3</scale>
				<Icon>
					<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
				</Icon>
				<hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
			</IconStyle>
			<LabelStyle>
				<scale>1.1</scale>
			</LabelStyle>
			<ListStyle>
			</ListStyle>
		</Style>
		<StyleMap id="sm_ylw-pushpin">
			<Pair>
				<key>normal</key>
				<styleUrl>#s_ylw-pushpin_0</styleUrl>
			</Pair>
			<Pair>
				<key>highlight</key>
				<styleUrl>#s_ylw-pushpin_1</styleUrl>
			</Pair>
		</StyleMap>
		
		<Style id="s_polygon">
			<IconStyle>
			</IconStyle>
			<LineStyle>
				<color>adffffff</color>
			</LineStyle>
			<PolyStyle>
				<color>66abffac</color>
			</PolyStyle>
		</Style>
		<Style id="s_polygon_light">
			<IconStyle>
			</IconStyle>
			<LineStyle>
				<color>66ffffff</color>
			</LineStyle>
			<PolyStyle>
				<color>33abffac</color>
			</PolyStyle>
		</Style>
	</Document>
</kml>
